  <tr>
    <td><%= link.name  %></td>
    <td><%= link_to link.url, link.url %></td>
    <td><small><%= link.created_at.localtime.strftime("%b %d, %Y")  %></small></td>
    <td>
      <%= link_to 'Edit', edit_web_link_path(link), :class => 'btn btn-mini' %>
      <%= link_to 'Destroy', '/links/'+link.key, method: :delete, :class => 'btn btn-mini btn-danger',
        data: { confirm: 'Are you sure?' } %>
    </td>
  </tr>